import sys

from PyQt5.QtWidgets import *

if __name__=='__main__':
#创建QApplication类的实例
    app=QApplication(sys.argv)
    #创建窗口
    w=QWidget()
    #设置窗口尺寸
    w.resize(400,200)
    #移到窗口
    w.move(300,300)

    #设置窗口的标题
    w.setWindowTitle('1111')
    #显示窗口
    w.show()

    #进入程序主循环，通过exit函数确保主循环安全结束
    sys.exit(app.exec_())
